{
  "extends": [
    "airbnb-base",
    "prettier"
  ],
  "env": {
    "node": true
  },
  "plugins": [
    "import",
    "prettier"
  ],
  "rules": {
    "class-methods-use-this": 0,
    "no-param-reassign": 0,
    "prettier/prettier": ["error", { "singleQuote": true }],
    "no-restricted-syntax": ["error", "LabeledStatement", "WithStatement"]
  }
}
